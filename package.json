{
  "name": "@based/template",
  "private": true,
  "version": "1.0.0",
  "description": "Template of a full scale application using Based",
  "main": "index.js",
  "type": "module",
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "scripts": {
    "test": "vitest",
    "dev": "npx @based/sdk dev",
    "dev-op": "./scripts/check1Password.sh && op run --env-file=\"./local.env\" --no-masking -- npx @based/sdk dev",
    "dev-cloud": "npx @based/sdk dev --cloud",
    "typecheck": "tsc --noEmit",
    "deploy": "npx @based/sdk deploy",
    "clean": "git -dfx"
  },
  "dependencies": {
    "@based/sdk": "1.0.2",
    "@based/docs": "^0.3.1",
    "@based/stories": "^0.3.3",
    "@based/ui": "^8.2.0-alpha.2",
    "@saulx/crypto": "^1.1.0",
    "inlines": "^0.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}

